<FBDSCENERIOS>
<!--user responsible to create sensible scenarios-->
<SCENERIO>
	<!-- SCENERIO 1 -->
	<TITLE>BOX ON TABLE</TITLE>
	<TEXT></TEXT>
	<TABLE>TRUE
		<ANGLE>15</ANGLE> <!-- in degrees-->
		<MU_STATIC>1</MU_STATIC>
		<MU_KINETIC>.4</MU_KINETIC>
	</TABLE>
	<BOX>
		<SIZE>80</SIZE>
		<ANGLE>15</ANGLE>
		<COLOR>RED</COLOR>
		<CONTACT>TRUE</CONTACT>
		<VELOCITY>
			<MAGNITUDE>0</MAGNITUDE>
			<ANGLE></ANGLE>
		</VELOCITY>
		<APPLIEDFORCE> FALSE
			<MAGNITUDE></MAGNITUDE>
			<ANGLE></ANGLE>
			<TENSION></TENSION>
		</APPLIEDFORCE>
	</BOX>
	<FIXED>FALSE</FIXED>
	<PULLEY>FALSE
		<BOX_SIZE></BOX_SIZE>
		<BOX_COLOR></BOX_COLOR>
	</PULLEY>
	
	<F_SCALING></F_SCALING>
	<GRAVITY></GRAVITY>
	
</SCENERIO>

